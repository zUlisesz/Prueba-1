public class Persona {
    private String nombre ;
    private int edad ;
    
    public Persona(String nombre , int edad){
        this.nombre = nombre ;
        this.edad = edad;
    }
    
    public Persona(){
        this.nombre = "unknown";
        this.edad = 10 ; 
    }
    
    public String getInfo(){
        return "Nombre: "+this.nombre+" - edad: "+this.edad ; 
    }
}
